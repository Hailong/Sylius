<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Sylius\Component\User\Model\UserMiniProgram" table="zshwag_user_mini_program">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="openId" type="string" column="open_id" length="255" unique="true"/>
    <field name="jsCode" type="string" column="js_code" length="255"/>
    <field name="sessionKey" type="string" column="session_key" length="255"/>
    <field name="unionId" type="string" column="union_id" length="255" nullable="true"/>
    <field name="lastLogin" type="integer" column="last_login"/>
    <many-to-one field="user" target-entity="Sylius\Component\User\Model\UserInterface" inversed-by="miniProgramAccounts">
        <join-column name="user_id" referenced-column-name="id" />
    </many-to-one>
  </mapped-superclass>
</doctrine-mapping>
