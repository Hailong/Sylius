<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="zshwag.http_client.class">Buzz\Client\Curl</parameter>
    </parameters>
    <services>
        <service id="zshwag.http_client" class="%zshwag.http_client.class%"/>
        <service id="security.authentication.listener.mini_program"
                 class="Zshwag\Bundle\WechatBundle\Security\Firewall\MiniProgramListener"
                 parent="security.authentication.listener.abstract"
                 abstract="true" />
        <service id="security.authentication.provider.mini_program"
                 class="Zshwag\Bundle\WechatBundle\Security\Authentication\Provider\MiniProgramProvider"
                 public="false" />
    </services>
</container>
